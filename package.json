{
    "name": "ketab-online-sdk",
    "description": "JavaScript SDK for accessing ketabonline.com books",
    "version": "1.1.0",
    "author": "Ragaeeb Haq",
    "license": "MIT",
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "source": "src/index.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./package.json": "./package.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ragaeeb/ketab-online-sdk.git"
    },
    "files": [
        "dist/*.js",
        "dist/*.mjs",
        "dist/*.d.ts",
        "dist/*.map"
    ],
    "scripts": {
        "build": "tsdown",
        "dev": "tsdown --watch",
        "lint": "biome check .",
        "format": "biome format .",
        "test": "bun test"
    },
    "keywords": [
        "ketabonline.com",
        "arabic",
        "islamic"
    ],
    "devDependencies": {
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@types/node": "^24.10.1",
        "@types/unzipper": "^0.10.11",
        "@biomejs/biome": "^2.3.5",
        "bun-types": "^1.3.2",
        "tsdown": "^0.16.4",
        "semantic-release": "^25.0.2",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "unzipper": "^0.12.3"
    },
    "packageManager": "bun@1.2.14",
    "engines": {
        "node": ">=23.0.0",
        "bun": ">=1.2.0"
    }
}
